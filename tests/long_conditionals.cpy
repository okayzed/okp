#include <iostream>

def main():
    a = 10
    b = 20
    c = 30

    // a joined conditional
    if a != 30 && \  
        b == 20 && c != 40 && \  
        b != 19:
      print "worked"

    // joining with and without spaces after backslash
    if a == 10 \
      && b == 20:
      print "a is 10 and b is 30"

    // a joined string
    print "the quick brown \
      fox"


